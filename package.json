{
  "name": "react-workbench",
  "version": "0.0.0",
  "description": "An environment for testing and building react components.",
  "main": "build/index.js",
  "scripts": {
    "build": "webpack --config config/webpack.js",
    "build:production": "NODE_ENV=production npm run build",
    "dev": "webpack-dev-server --hot --config config/webpack.js",
    "test": "NODE_ENV=test jest",
    "test:watch": "npm run test -- --watch",
    "test:coverage": "npm run test -- --coverage",
    "test:coverage:report": "npm run test:coverage && cat ./coverage/lcov.info | ./node_modules/coveralls/bin/coveralls.js"
  },
  "author": "DataScience Engineering",
  "license": "ISC",
  "devDependencies": {
    "babel-core": "^6.24.1",
    "babel-loader": "^6.4.1",
    "coverage": "^0.0.0",
    "html-webpack-plugin": "^2.28.0",
    "react-hot-loader": "next",
    "webpack": "^2.4.1",
    "webpack-dev-server": "^2.4.2"
  },
  "dependencies": {
    "babel-plugin-transform-decorators-legacy": "^1.3.4",
    "babel-preset-es2015": "^6.24.1",
    "babel-preset-react": "^6.24.1",
    "babel-preset-stage-1": "^6.24.1",
    "react": "^15.5.4",
    "react-dom": "^15.5.4"
  }
}
